{
  "name": "converttoapppoui",
  "displayName": "Convert To App - POUI",
  "description": "Extens√£o para converter projeto PO-UI em .app para Protheus.",
  "version": "0.0.2",
  "publisher": "KaironFerreira",
  "author": {
    "name": "Kairon Ferreira",
    "email": "gelsonkairon.10@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/kaironferreira/converttoapppoui",
  "icon": "assets/icons/logo-app.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaironferreira/converttoapppoui.git"
  },
  "keywords": [
    "protheus",
    "advpl",
    "poui",
    "angular",
    "totvs"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.convertToApp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertToApp",
        "title": "Convert to APP - POUI"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.convertToApp",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "archiver": "^7.0.1"
  }
}